{
    "env": {
        "browser": true,
        "es2017": true,
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        //"ecmaVersion": 2017,
        "sourceType": "module"
    },
    "globals": {
        "jQuery": "readonly",
        "$": "readonly"
    },
    "rules": {
        "no-undef": ["off"],
        "semi": ["error", "always", {"omitLastInOneLineBlock": true}]
    }
}