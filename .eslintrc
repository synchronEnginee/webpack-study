{
    "env": {
        "browser": true,
        "es2017": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        //"ecmaVersion": 2015
        "sourceType": "module"
    },
    "globals": {
        "utils": "readonly",
        "jQuery": "readonly",
        "$": "readonly"
    },
    "rules": {
        "no-undef": ["off"],
        "semi": ["error", "never"]
    }
}